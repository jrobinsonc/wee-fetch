{
  "name": "wee-fetch",
  "version": "1.1.0",
  "description": "Isomorphic promise-based HTTP client for the browser, Node and React Native.",
  "license": "MIT",
  "author": "Jose Robinson <jrobinsonc@gmail.com> (https://joserobinson.com)",
  "files": [
    "dist"
  ],
  "main": "./dist/wee-fetch.cjs.js",
  "module": "./dist/wee-fetch.es.js",
  "exports": {
    ".": {
      "import": "./dist/wee-fetch.es.js",
      "require": "./dist/wee-fetch.cjs.js"
    }
  },
  "scripts": {
    "prebuild": "npm run lint && rimraf dist",
    "build": "vite build",
    "lint": "eslint .; tsc --noEmit ",
    "fix": "eslint --fix .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrobinsonc/wee-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/jrobinsonc/wee-fetch/issues"
  },
  "homepage": "https://github.com/jrobinsonc/wee-fetch#readme",
  "keywords": [
    "ajax",
    "api",
    "async",
    "await",
    "browser",
    "fetch",
    "http",
    "isomorphic",
    "native",
    "node",
    "polyfill",
    "ponyfill",
    "promise",
    "react-native",
    "rest",
    "restful",
    "universal",
    "url",
    "wrapper",
    "xhr"
  ],
  "dependencies": {
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.8.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^27.5.1",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "vite": "^2.8.0",
    "vite-plugin-banner": "^0.2.0"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 7"
  }
